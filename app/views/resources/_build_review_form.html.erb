<% content_for(:scripts) do %>
  <%= javascript_include_tag 'star_rating' %>`
<% end %>

<% @review = resource.reviews.build  %>

<%= form_for @review, class: "form-group" do |f| %>
  <%= render '/reviews/form', f: f %>
  <%= f.hidden_field :resource_id, :value => resource.id %>
    <div class="col-md-12">
      <%= f.submit "Create review", class: "btn btn-primary btn-main pull-right" %>
    <div clas="col-md-12">
<% end %>
