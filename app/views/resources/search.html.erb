<div class="row">
  <div class="col-md-8 col-md-offset-1">
       <div class="row">
        <div class="col-md-6">
  <!--         <h4><%#= @resources.length %> Results Found</h4> -->
        </div>
        <div class="col-md-6">
          <ul class="list-inline list-style pull-right">
            <li><div class="btn btn-default sort-btn"><%= sortable "name" %></li>
            <li><div class="btn btn-default sort-btn"><%= sortable "created_at", "Date Added" %></div></li>
            <li><div class="btn btn-default sort-btn"><%= sortable "average_score", "Score" %></div></li>
            <li><div class="btn btn-default sort-btn"><%= sortable "cost" %></div></li>
          </ul>
        </div>
      </div>
    <table class="table">
      <%= render @results %>
    </table>
    <%= will_paginate @results %>
  </div>
  <div class="col-md-2 panel-padding">
    <%= render 'tag_filter' %>
  </div>
</div>