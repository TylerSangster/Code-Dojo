<div class="container">

  <% provide(:title, 'Edit Resource') %>

  <%= form_for @resource do |f| %>
    <%= render 'form', f: f %>
    
    <div><%= f.check_box :remove_resource_photo %><%= f.label :remove_resource_photo %>
    </div>

    <div>
      <%= f.submit "Save Changes" %>
    </div>
    
    <% end %>

</div>