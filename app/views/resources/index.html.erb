<div class="row">
  <div class="col-md-8 col-md-offset-1">
     <div class="row">
      <div class="col-md-6">
<!--         <h4><%#= @resources.length %> Results Found</h4> -->
      </div>
      <div class="col-md-6">
        <div class="btn-group pull-right">
          <div class="btn btn-default"><%= sortable "name" %></div>
          
          <div class="btn btn-default"><%= sortable "created_at", "Most Recent" %></div>

          <div class="btn btn-default"><%= sortable "average_score", "Score" %></div>
          <div class="btn btn-default"><%= sortable "cost" %></div>
        </ul>
      </div>
    </div>
    <table class="table">
    <%= render @resources %>
    </table>
    <h4 class="text-center"><%= will_paginate @resources %></h4>
  </div>
  <div class="col-md-2 panel-padding">
    <%= render 'tag_filter' %>
  </div>
</div>
