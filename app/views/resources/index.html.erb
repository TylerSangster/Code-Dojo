<div class="row">
  <div class="col-md-8 col-md-offset-1">
     <div class="row">
      <div class="col-md-6">
<!--         <h4><%#= @resources.length %> Results Found</h4> -->
      </div>
      <div class="col-md-6">
        <ul class="list-inline list-style pull-right">
          <li><%= sortable "name" %></li>
          <li><%= sortable "created_at", "Date Added" %></li>
          <li><%= sortable "average_score", "Score" %></li>
          <li><%= sortable "cost" %></li>
        </ul>
      </div>
    </div>
    <table class="table">
    <%= render @resources %>
    </table>
    <h4 class="text-center"><%= will_paginate @resources %></h4>
  </div>
  <div class="col-md-2 panel-padding">
    <%= render 'tag_filter' %>
  </div>
</div>
