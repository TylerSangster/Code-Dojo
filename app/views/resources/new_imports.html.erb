<% provide(:title, '') %>

<div class="form-signin">
  <h2 class="form-signin-heading">Import Products</h2><br />
  <%= form_tag import_resources_path, multipart: true do %>
    <div class="fileupload fileupload-new" data-provides="fileupload">

      <%= file_field_tag :file, class:"fileupload fileupload-new " %>

    </div>

    <div><%= submit_tag "Import",class: "btn btn-lg btn-primary btn-block" %></div>
  <% end %>
</div>
<br /><br /><br /><br /><br /><br /><br />
<div class="form-signin">
  <h2 class="form-signin-heading">Import Reviews</h2><br />
  <%= form_tag import_reviews_path, multipart: true do %>
    <div class="fileupload fileupload-new" data-provides="fileupload">

      <%= file_field_tag :file, class:"fileupload fileupload-new " %>

    </div>

    <div><%= submit_tag "Import",class: "btn btn-lg btn-primary btn-block" %></div>
  <% end %>
</div>


