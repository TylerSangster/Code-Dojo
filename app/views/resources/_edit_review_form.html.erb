<div class="row">
  <% content_for(:scripts) do %>
    <%= javascript_include_tag 'star_rating' %>`
  <% end %>

  <%= form_for @review, class: "form-group" do |f| %>
    <%= render '/reviews/form', f: f %>


    <div class="col-md-12">
      <%= f.submit "Update review", class: "btn btn-primary btn-main pull-right" %>
    <div clas="col-md-12">
  <% end %>

</div>
