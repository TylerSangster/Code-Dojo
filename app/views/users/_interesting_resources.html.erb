<p>Resources this user is interested in:</p>
<table class="table">
  <% @user.interesting_resources.each do |interesting_resource| %>
    <tr>
      <td>
      <%= link_to interesting_resource.name, interesting_resource %><i class="pull-right <%= star_classes(interesting_resource.average_score.to_f) %>"></i>
      </td>
    </tr>
  <% end %>
</table>
